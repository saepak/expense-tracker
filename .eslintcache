ess,\n    onFailure,\n  })\n\n  const reload = () => {\n    window.location.reload(false);\n  }\n\n  return (\n    <>\n      <Button \n        onClick={signOut}\n        className='logoutBtn'\n      >\n        <Title level={5}>\n          <LogoutOutlined style={{paddingRight: 10}}/>Sign out\n        </Title>\n      </Button>\n    </>\n  )\n}\n\nexport default Logout","/Users/saebyeol/Documents/saebyeol/00_practice/react/expense-tracker/src/components/oauth/LoginButton.js",[],{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","severity":1,"message":"84","line":2,"column":17,"nodeType":"85","messageId":"86","endLine":2,"endColumn":26},{"ruleId":"83","severity":1,"message":"87","line":2,"column":28,"nodeType":"85","messageId":"86","endLine":2,"endColumn":36},{"ruleId":"88","severity":1,"message":"89","line":1,"column":1,"nodeType":"90","endLine":16,"endColumn":2},{"ruleId":"83","severity":1,"message":"91","line":10,"column":7,"nodeType":"85","messageId":"86","endLine":10,"endColumn":17},{"ruleId":"83","severity":1,"message":"87","line":1,"column":16,"nodeType":"85","messageId":"86","endLine":1,"endColumn":24},{"ruleId":"83","severity":1,"message":"92","line":5,"column":8,"nodeType":"85","messageId":"86","endLine":5,"endColumn":19},"no-native-reassign",["93"],"no-negated-in-lhs",["94"],"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","'useState' is defined but never used.","import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration","'tailLayout' is assigned a value but never used.","'LoginButton' is defined but never used.","no-global-assign","no-unsafe-negation"]